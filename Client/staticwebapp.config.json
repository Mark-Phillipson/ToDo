{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/*",
            "headers": {
                "cache-control": "must-revalidate, max-age=3600"
            }
        },
        {
            "route": "/sample-data/*",
            "headers": {
                "cache-control": "public, max-age=86400"
            }
        },
        {
            "route": "/_framework/*",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/css/*",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/js/*",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        }
    ]
}